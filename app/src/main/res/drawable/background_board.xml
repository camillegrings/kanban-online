<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#a4bbbbbb"/>
            <corners android:radius="2dp" />
        </shape>
    </item>

    <item
        android:left="0dp"
        android:right="2dp"
        android:top="0dp"
        android:bottom="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#a4b1d9a5"/>
            <corners android:radius="2dp" />
            <stroke
                android:width="1dp"
                android:color="#a47c9f72" />
        </shape>
    </item>
</layer-list>
